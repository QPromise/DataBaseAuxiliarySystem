package cn.edu.ouc.pojo.vo;

import java.io.Serializable;
import java.util.Arrays;

public class FoldLineDiagram implements Serializable {

	private static final long serialVersionUID = 1L;

	private String title;
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	private String[] transverseValues;
	private Double[] values;
	public String[] getTransverseValues() {
		return transverseValues;
	}
	public void setTransverseValues(String[] transverseValues) {
		this.transverseValues = transverseValues;
	}
	public Double[] getValues() {
		return values;
	}
	public void setValues(Double[] values) {
		this.values = values;
	}
	@Override
	public String toString() {
		return "FoldLineDiagram [title=" + title + ", transverseValues=" + Arrays.toString(transverseValues)
				+ ", values=" + Arrays.toString(values) + "]";
	}
}
